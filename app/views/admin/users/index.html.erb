<% provide :title, t("admin.users.title") %>
<h1><%= t "admin.users.title" %></h1>
<div class="well">
<div>
    <div class="col-md-3">
      <%= label_tag t(".search") %>
      <%= text_field_tag :search, params[:search], class: "form-control" %>
    </div>
    <div class="pull-right">
      <%= button_tag :search, id: "btn-search", class: "btn btn-default btn-sm" %>
    </div>
</div>
<div id="result">
  <%= render partial: "admin/users/user", locals: {users: @users} %>
</div>
